        <TextBox Name="txtNameCityCounty" HorizontalAlignment="Left" Height="23" Margin="420,38,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="370"/>
        <TextBox Name="txtCity" HorizontalAlignment="Left" Height="23" Margin="420,66,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="370"/>
        <TextBox Name="txtCounty" HorizontalAlignment="Left" Height="23" Margin="420,94,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="370"/>
    </Grid>
</UserControl>
